msgid "Accepted"
msgstr "已接受"

#: plugins/omemo/src/ui/encryption_preferences_entry.vala:227
msgid "Rejected"
msgstr "已拒绝"

#: plugins/omemo/src/ui/encryption_preferences_entry.vala:230
msgid "Verified"
msgstr "已验证"

#: plugins/omemo/src/ui/encryption_preferences_entry.vala:279
msgid "New device"
msgstr "新设备"

